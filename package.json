{
  "author": "Pedro Costa <pedrohenriqueteodorodacosta@gmail.com>",
  "contributors": [
    "Florian Imdahl <git@ffflorian.de>"
  ],
  "description": "Create Windows shortcuts using a VBScript.",
  "devDependencies": {
    "@ffflorian/eslint-config": "0.6.2",
    "@ffflorian/prettier-config": "0.1.2",
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/git": "9.0.0",
    "@types/node": "~14",
    "@typescript-eslint/eslint-plugin": "4.0.0",
    "@typescript-eslint/parser": "3.10.1",
    "babel-eslint": "10.1.0",
    "eslint": "7.7.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-jasmine": "4.1.1",
    "eslint-plugin-jsdoc": "30.3.0",
    "eslint-plugin-no-unsanitized": "3.1.2",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-simple-import-sort": "5.0.3",
    "eslint-plugin-sort-keys-fix": "1.1.1",
    "eslint-plugin-typescript-sort-keys": "1.3.0",
    "husky": "4.2.5",
    "jasmine": "3.6.1",
    "lint-staged": "10.2.13",
    "prettier": "2.1.1",
    "rimraf": "3.0.2",
    "semantic-release": "17.1.1",
    "typescript": "4.0.2"
  },
  "engines": {
    "node": ">= 10.9"
  },
  "files": [
    "dist",
    "scripts"
  ],
  "homepage": "https://github.com/ffflorian/windows-shortcut-maker",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "keywords": [
    "windows",
    "shortcut",
    "native",
    "lightweight",
    "desktop",
    "lnk",
    "url"
  ],
  "license": "GPL-3.0",
  "lint-staged": {
    "*.{json,md}": [
      "prettier --write"
    ],
    "src/**/*.ts": [
      "eslint --fix"
    ]
  },
  "main": "dist/index.js",
  "name": "@ffflorian/windows-shortcut-maker",
  "prettier": "@ffflorian/prettier-config",
  "repository": "https://github.com/ffflorian/windows-shortcut-maker.git",
  "scripts": {
    "build": "tsc",
    "clear": "rimraf dist",
    "dist": "yarn clear && yarn build",
    "fix": "yarn fix:other && yarn fix:ts",
    "fix:other": "yarn prettier --write",
    "fix:ts": "yarn lint:ts --fix",
    "lint": "yarn lint:ts && yarn lint:other",
    "lint:other": "yarn prettier --list-different",
    "lint:ts": "eslint --ignore-path .gitignore --ext .ts src/",
    "prettier": "prettier \"*.{json,md}\"",
    "release": "semantic-release",
    "test": "exit 0"
  },
  "version": "2.0.1"
}
